{
  "name": "debixy/plugins",
  "description": "Utility package for laravel application.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Adebisi",
      "email": "debxydev@gmail.com",
      "homepage": "https://github.com/debixy",
      "role": "Developer"
    }
  ],
  "require": {
    "illuminate/support": "^10.0",
    "symfony/process": "^6.0",
    "illuminate/console": "^10.0",
    "illuminate/filesystem": "^10.0",
    "illuminate/view": "^10.0",
    "illuminate/auth": "^10.0"
  },
  "require-dev": {
    "laravel/pint": "^1.13"
  },
  "autoload": {
    "psr-4": {
      "Debixy\\Plugins\\": "src/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "Debixy\\Plugins\\PluginsServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
